{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "plugin:react/recommended", //
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": 0,
    "one-var": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "object-curly-newline": 0,
    "no-restricted-syntax": 0
  }
}
